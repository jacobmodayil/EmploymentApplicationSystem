<table class="table table-condensed table-hover">
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>Action</th>
    </tr>
  </thead>

  <tbody>
    <% @applicants.each do |applicant| %>
      <tr>
        <td><%= applicant.first_name %></td>
        <td><%= applicant.last_name %></td>
        <td><%= applicant.email %></td>
        <td><%= link_to 'Review Application', jobs_viewapplicants_path(:job_id => job.id,:admin_email => @email_id ), class: "btn btn-link" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
